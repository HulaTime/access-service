{
  "name": "access",
  "version": "1.0.0",
  "description": "Marketplace Access Service",
  "main": "src/index.ts",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc && cp api.yaml dist",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "test": "node_modules/.bin/jest --detectOpenHandles --collectCoverage",
    "npm-check": "npm-check -u",
    "prestart": "npm run build",
    "start": "node dist/src/index.js",
    "setupDb": "PGPASSWORD=myspoke psql -h 0.0.0.0 -p 54320 -U myspoke -d myspoke -c 'create schema access;'",
    "migrations:up": "typeorm-ts-node-commonjs migration:run -d ./db/mig-datasource.ts",
    "migrations:down": "typeorm-ts-node-commonjs migration:revert -d ./db/mig-datasource.ts",
    "migrations:create": "typeorm migration:create",
    "premigrations:generate": "npm run build",
    "migrations:generate": "typeorm-ts-node-commonjs migration:generate -d ./db/mig-datasource.ts -p",
    "generate:docs": "typedoc --plugin typedoc-plugin-markdown  src/index.ts",
    "generate:types": "openapi-typescript api.yaml --output types/api.d.ts",
    "generate:tokenKeys": "ssh-keygen -b 521 -t ECDSA -m PEM -f ./token-key -N elephant",
    "postgenerate:tokenKeys": "ssh-keygen -e -m PEM -f ./token-key.pub > ./pem-token-key.pub",
    "generate:tokenKeysNoPass": "ssh-keygen -b 521 -t ECDSA -m PEM -f ./token-key",
    "postgenerate:tokenKeysNoPass": "ssh-keygen -e -m PEM -f ./token-key.pub > ./pem-token-key.pub",
    "rm:tokenKeys": "rm ./token-key* && rm ./pem-token*"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/bunyan": "^1.8.8",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "eslint": "^7.32.0",
    "eslint-config-sensible-style": "^0.0.7",
    "jest": "^29.2.0",
    "npm-check": "^6.0.1",
    "openapi-typescript": "^5.4.1",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.8",
    "typedoc-plugin-markdown": "^3.13.4",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "argon2": "^0.30.1",
    "body-parser": "^1.20.1",
    "bunyan": "^1.8.15",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-openapi-validator": "^4.13.8",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "serialize-error": "^8.1.0",
    "typeorm": "^0.3.10",
    "uuid": "^9.0.0"
  }
}
