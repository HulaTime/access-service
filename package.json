{
  "name": "access",
  "version": "1.0.0",
  "description": "Marketplace Access Service",
  "main": "src/index.ts",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "generate-docs": "typedoc --plugin typedoc-plugin-markdown  index.ts",
    "lint": "eslint **/*.ts **/*.json",
    "test": "node_modules/.bin/jest --detectOpenHandles --collectCoverage --forceExit",
    "npm-check": "npm-check -u",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "migrations:up": "typeorm-ts-node-commonjs migration:run -d ./db/mig-datasource.ts",
    "migrations:down": "typeorm-ts-node-commonjs migration:revert -d ./db/mig-datasource.ts",
    "migrations:create": "typeorm migration:create",
    "premigrations:generate": "npm run build",
    "migrations:generate": "typeorm-ts-node-commonjs migration:generate -d ./db/mig-datasource.ts -p",
    "postmigrations:generate": "npm run migrations:up"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/bunyan": "^1.8.8",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^7.32.0",
    "eslint-config-sensible-style": "^2.0.0",
    "jest": "^29.2.0",
    "npm-check": "^6.0.1",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.8",
    "typedoc-plugin-markdown": "^3.13.4",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "bunyan": "^1.8.15",
    "express": "^4.18.2",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "serialize-error": "^8.1.0",
    "typeorm": "^0.3.10"
  }
}
